import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner input=new Scanner(System.in);
        System.out.println("Total Users: ");
        int total_user=input.nextInt();
        System.out.println("Staff users: ");
        int staff_users=input.nextInt();
        int non_tech=staff_users/3;
        int student_user=total_user-(staff_users+non_tech);
        System.out.println("Student Users: "+student_user);
    }
}
INPUT:
Total Users: 856
Staff users: 126
OUTPUT:
Student Users: 688
